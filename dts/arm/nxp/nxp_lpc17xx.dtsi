#include <arm/armv7-m.dtsi>

/ {
	cpus {
		#address-cells = <1>;
		#size-cells = <0>;

		cpu@0 {
			compatible = "arm,cortex-m3";
			reg = <0>;
		};
	};

	sram0:memory@2007C000 {
		compatible = "mmio-sram";
		reg = <0x2007C000 0x4000>;
	};

	soc {
		flash0:flash@0 {
			reg = <0 0x80000>;
		};
	};
};

&nvic {
	arm,num-irq-priority-bits = <3>;
};
